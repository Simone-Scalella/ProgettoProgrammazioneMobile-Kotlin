<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.esamepm">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.EsamePM">
        <activity android:name="homeGen.MainActivityGen"
            android:screenOrientation="portrait"></activity>
        <activity android:name="homeCuoco.MainActivityCuo"
            android:screenOrientation="portrait"/>
        <activity android:name="homeProf.mainActivityPro"
            android:screenOrientation="portrait"/>
        <activity android:name="welcome.MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>