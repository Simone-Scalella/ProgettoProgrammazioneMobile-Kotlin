<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context="homeProf.macchine.inserimento.InserisciPrenotazioneFragment">

    <TextView
        android:id="@+id/textViewOraPrenotazione"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="316dp"
        android:layout_marginBottom="8dp"
        android:text="Ora"
        app:layout_constraintBottom_toTopOf="@+id/mesePrenotazione"
        app:layout_constraintStart_toStartOf="parent" />

    <com.google.android.material.textfield.TextInputEditText
        android:id="@+id/oraPrenotazione"
        android:layout_width="105dp"
        android:layout_height="47dp"
        android:layout_marginStart="296dp"
        android:layout_marginTop="128dp"
        android:hint="00:00"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/inputCodiceMaccPrenotazione" />

    <com.google.android.material.textfield.TextInputEditText
        android:id="@+id/annoPrenotazione"
        android:layout_width="104dp"
        android:layout_height="51dp"
        android:layout_marginStart="152dp"
        android:layout_marginTop="128dp"
        android:hint="anno"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/inputCodiceMaccPrenotazione" />

    <com.google.android.material.textfield.TextInputEditText
        android:id="@+id/mesePrenotazione"
        android:layout_width="59dp"
        android:layout_height="51dp"
        android:layout_marginStart="76dp"
        android:layout_marginTop="128dp"
        android:hint="mese"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/inputCodiceMaccPrenotazione" />

    <com.google.android.material.textfield.TextInputEditText
        android:id="@+id/giornoPrenotazione"
        android:layout_width="59dp"
        android:layout_height="51dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="128dp"
        android:hint="giorno"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/inputCodiceMaccPrenotazione" />

    <com.google.android.material.textfield.TextInputEditText
        android:id="@+id/inputCodiceMaccPrenotazione"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="0dp"
        android:layout_marginTop="204dp"
        android:hint="Codice della macchina"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <com.google.android.material.textfield.TextInputEditText
        android:id="@+id/InputDurata"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="0dp"
        android:layout_marginTop="276dp"
        android:hint="Durata ( in ore )"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textViewStartPrenotazione"
        android:layout_width="411dp"
        android:layout_height="64dp"
        android:layout_marginTop="60dp"
        android:text="Completa i campi riportati di seguito "
        android:textAlignment="center"
        android:textSize="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textViewGiornoPrenotazione"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginBottom="8dp"
        android:text="Giorno"
        app:layout_constraintBottom_toTopOf="@+id/giornoPrenotazione"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textViewMesePrenotazione"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="76dp"
        android:layout_marginBottom="8dp"
        android:text="Mese"
        app:layout_constraintBottom_toTopOf="@+id/mesePrenotazione"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textViewAnnoPrenotazione"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="176dp"
        android:layout_marginBottom="8dp"
        android:text="Anno"
        app:layout_constraintBottom_toTopOf="@+id/annoPrenotazione"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/buttonPrenotazione"
        android:layout_width="411dp"
        android:layout_height="70dp"
        android:layout_marginBottom="148dp"
        android:text="Invia la richiesta"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>